\ProvidesPackage{logic}

\renewcommand{\And}[2]{\left ( #1 \right ) \wedge \left ( #2 \right )}
\newcommand{\Coimplies}[2]{\left ( #1 \right ) \Leftrightarrow \left ( #2 \right )}
\newcommand{\Exists}[2]{\left ( \exists #1 \right ) \left ( #2 \right )}
\newcommand{\ExistsOne}[2]{\left ( \exists ! #1 \right ) \left ( #2 \right )}
\newcommand{\ForAll}[2]{\left ( \forall #1 \right ) \left ( #2 \right )}
\newcommand{\Formula}{\mathcal{F}}
\newcommand{\Implies}[2]{\left ( #1 \right ) \Rightarrow \left ( #2 \right )}
\newcommand{\Litteral}{\mathcal{L}}
\newcommand{\Not}[1]{\neg \left ( #1 \right )}
\newcommand{\Or}[2]{\left ( #1 \right ) \vee \left ( #2 \right )}
\newcommand{\Xor}[2]{\left ( #1 \right ) \mathring{\vee} \left ( #2 \right )}
