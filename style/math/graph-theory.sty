\newcommand{\GraphNeighborhoodIn}[1]{\mathcal{N}^-\left ( #1 \right )}
\newcommand{\GraphNeighborhoodOut}[1]{\mathcal{N}^+\left ( #1 \right )}
\newcommand{\Node}{n}
\newcommand{\Nodes}{\mathcal{N}}
\newcommand{\Edge}{e}
\newcommand{\Edges}{\mathcal{E}}
\newcommand{\Root}{r}
